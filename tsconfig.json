{
	"compilerOptions": {
		"target": "ES5",
		"module": "ESNext",
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"allowJs": true,
		"jsx": "react-jsx",
		"baseUrl": "./",
		"moduleResolution": "Node",
		"sourceMap": true,
		"esModuleInterop": true,
		"strict": true,
		"removeComments": true,
		"strictFunctionTypes": true,
		"skipLibCheck": true
	},
	"include": ["src/@types"],
	"exclude": ["node_modules"]
}
